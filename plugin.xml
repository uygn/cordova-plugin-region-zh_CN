<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
		id="cordova-plugin-region-zh_CN"
		version="0.1">
	<name>CFBundleDevelopmentRegion-zh_CN</name>
	<description>Changes the iOS CFBundleDevelopmentRegion to zh_CN in *-Info.plist.</description>
	<license>Apache 2.0</license>

	<platform name="ios">
		<config-file target="*-Info.plist" parent="CFBundleDevelopmentRegion">
			<string>zh_CN</string>
		</config-file>
	</platform>

</plugin>